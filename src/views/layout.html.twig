<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- FAVICON -->
    <link rel="icon" type="image/png" href="../public/assets/img/favico.png" sizes="32x32"/>
    <link href="../public/assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="../public/assets/css/stylesheet.css" rel="stylesheet">

    {% block css %} {% endblock %}
    <title>Gabriel Mattei | {% block title %} index {% endblock %}</title>

</head>
<body>

{% include 'partial/header.html.twig' %}

{% block content %}

{% endblock %}

{% include 'partial/footer.html.twig' %}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<script src="../public/assets/js/bootstrap.min.js"></script>
<script>
    $(document).ready(function () {
        $("[id^=year]").click(function () {
            annee = $(this).attr('id').replace('year', '');

            $(".content" + annee).fadeToggle();
        });
    });

    $(document).ready(function () {
        $("#symf").click(function () {
            $(".symphonicMusic").css("visibility", 'visible');
            $(".performingArt").css("visibility", 'hidden');
            $(".performing").css("opacity", '0')
            $(".musique").css("opacity", '1')


        });

        $("#perfo").click(function () {

            $(".performingArt").css("visibility", 'visible');
            $(".symphonicMusic").css("visibility", 'hidden');
            $(".musique").css("opacity", '0')
            $(".performing").css("opacity", '1')
        });
    });
</script>
</body>
</html>